\n  const [token, setToken] = useState(getToken);\n\n  const login = (userData: userData, history: History) => {\n    api\n      .post(\"/login\", userData)\n      .then((res) => {\n        localStorage.clear();\n        localStorage.setItem(\"@iCash:token\", res.data.accessToken);\n        setToken(res.data.accessToken);\n        history.push(\"/dashboard\");\n        toast.success(\"Usuário logado com sucesso!\");\n      })\n      .catch((err) => {\n        console.log(\"ERRO\", err);\n        toast.error(\n          \"Verifique seus dados. Caso seja um novo usuário, crie sua conta.\"\n        );\n      });\n  };\n\n  const loginAdmin = (userData: userData, history: History) => {\n    api\n      .post(\"/login\", userData)\n      .then((res) => {\n        console.log(res);\n        localStorage.clear();\n        localStorage.setItem(\"@iCash:token\", res.data.accessToken);\n        setToken(res.data.accessToken);\n        history.push(\"/admin-profile\");\n        toast.success(\"Admin logado\");\n      })\n      .catch((err) => {\n        console.log(\"ERRO\", err);\n        toast.error(\"Algo saiu mal. Tente novamente.\");\n      });\n  };\n\n  return (\n    <AuthContext.Provider value={{ login, token, setToken, loginAdmin }}>\n      {children}\n    </AuthContext.Provider>\n  );\n};\n\nexport const useAuth = () => useContext(AuthContext);\n","/home/pierre/Documentos/projects/Q2/sprint6/icash/src/providers/Register/index.tsx",[],"/home/pierre/Documentos/projects/Q2/sprint6/icash/src/types/userLoginData.ts",[],"/home/pierre/Documentos/projects/Q2/sprint6/icash/src/pages/Login/index.tsx",[],"/home/pierre/Documentos/projects/Q2/sprint6/icash/src/pages/Home/index.tsx",[],"/home/pierre/Documentos/projects/Q2/sprint6/icash/src/pages/Loyalty/index.tsx",[],"/home/pierre/Documentos/projects/Q2/sprint6/icash/src/pages/Register/index.tsx",[],"/home/pierre/Documentos/projects/Q2/sprint6/icash/src/services/api.tsx",[],"/home/pierre/Documentos/projects/Q2/sprint6/icash/src/pag